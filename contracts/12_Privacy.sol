pragma solidity ^0.6.0;


contract Key {
    uint256 public input = uint256(0xd40bfdeea10e7cd94d6e46732c3f34d91bbd10464f22e22c55731de705df1522);
    bytes32 public bajts32 = bytes32(input);
    bytes16 public bajts16 = bytes16(bajts32); // "0xd40bfdeea10e7cd94d6e46732c3f34d9"
}

contract Privacy {

  bool public locked = true;
  uint256 public ID = block.timestamp;
  uint8 private flattening = 10;
  uint8 private denomination = 255;
  uint16 private awkwardness = uint16(now);
  bytes32[3] private data;

  constructor(bytes32[3] memory _data) public {
    data = _data;
  }
  
  function unlock(bytes16 _key) public {
    require(_key == bytes16(data[2]));
    locked = false;
  }

